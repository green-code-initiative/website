  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.4",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.7.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "prettier": "^3.8.1",
    "sass": "~1.97.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-ssg": "^28.3.0",
    "vite-ssg-sitemap": "^0.10.0",
