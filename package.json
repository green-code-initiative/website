{
  "name": "green-code-initiative.org",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && npm run build-only",
    "preview": "vite preview",
    "build-only": "vite-ssg build",
    "lint": "eslint \"src/**/*.{js,ts,vue}\"",
